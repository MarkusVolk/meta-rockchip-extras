 		  DEPENDENT_SLICE_SEGMENTS_ENABLED_FLAG);
 	WRITE_PPS(!!(pps->flags & V4L2_HEVC_PPS_FLAG_OUTPUT_FLAG_PRESENT),
 		  OUTPUT_FLAG_PRESENT_FLAG);
