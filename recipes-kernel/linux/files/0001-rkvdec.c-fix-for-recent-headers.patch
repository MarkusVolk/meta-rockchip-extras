 	WRITE_PPS(pps->pic_parameter_set_id, PIC_PARAMETER_SET_ID);
 	WRITE_PPS(sps->seq_parameter_set_id, PPS_SEQ_PARAMETER_SET_ID);
-	WRITE_PPS(!!(pps->flags & V4L2_HEVC_PPS_FLAG_DEPENDENT_SLICE_SEGMENT),
